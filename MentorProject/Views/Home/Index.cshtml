@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers;
@{
    ViewData["Title"] = "Home Page";
}
<div class="card mx-auto  mt-4" >
 
    <div class="card-body">
        <h5 class="card-title">@ViewBag.Header</h5>
        <h6 class="card-subtitle mb-2 text-body-secondary">Card subtitle</h6>
        <p class="card-text">@ViewBag.GroupMemberStatus</p>
        <p class="card-text">@ViewData["Content"]</p>
        <span class="text-center @TempData["class-name"]">@TempData["CountOfStudents"]</span>
        <a href="@Url.Action("About","Home")" class="btn btn-sm btn-primary mx-2`">Go to About</a>
    </div>
</div>
